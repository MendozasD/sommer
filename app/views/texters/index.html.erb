<p id="notice"><%= notice %></p>

<h1>Texters</h1>

<table>
  <thead>
    <tr>
      <th>Piece</th>
      <th>Genre</th>
      <th>Year</th>
      <th>Responsible</th>
      <th>Comment</th>
      <th>Premiere</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @texters.each do |texter| %>
      <tr>
        <td><%= texter.piece %></td>
        <td><%= texter.genre %></td>
        <td><%= texter.year %></td>
        <td><%= texter.responsible %></td>
        <td><%= texter.comment %></td>
        <td><%= texter.premiere %></td>
        <td><%= link_to 'Show', texter %></td>
        <td><%= link_to 'Edit', edit_texter_path(texter) %></td>
        <td><%= link_to 'Destroy', texter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Texter', new_texter_path %>
