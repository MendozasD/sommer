<p id="notice"><%= notice %></p>

<h1>Directors</h1>

<table>
  <thead>
    <tr>
      <th>Piece</th>
      <th>Location</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Url</th>
      <th>Musiker</th>
      <th>Choreo</th>
      <th>Premiere</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @directors.each do |director| %>
      <tr>
        <td><%= director.piece %></td>
        <td><%= director.location %></td>
        <td><%= director.start_date %></td>
        <td><%= director.end_date %></td>
        <td><%= director.url %></td>
        <td><%= director.musiker %></td>
        <td><%= director.choreo %></td>
        <td><%= director.premiere %></td>
        <td><%= link_to 'Show', director %></td>
        <td><%= link_to 'Edit', edit_director_path(director) %></td>
        <td><%= link_to 'Destroy', director, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Director', new_director_path %>
